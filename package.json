{
  "name": "cfpathcheck",
  "xo": {
    "env": [
      "node"
    ],
    "prettier": true
  },
  "description": "Check CFML files for correct paths in cfinclude/cfimport tags",
  "version": "3.0.6",
  "homepage": "https://github.com/timbeadle/cfpathcheck",
  "author": {
    "name": "Tim Beadle",
    "url": "https://github.com/timbeadle"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timbeadle/cfpathcheck.git"
  },
  "bugs": {
    "url": "https://github.com/timbeadle/cfpathcheck/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/timbeadle/cfpathcheck/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "cli",
    "command line",
    "cfml",
    "static analysis"
  ],
  "preferGlobal": true,
  "bin": {
    "cfpathcheck": "./bin/cfpathcheck"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "snyk test --dev && npm run lint",
    "lint": "xo lib/cfpathcheck.js bin/cfpathcheck",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "checkstyle-formatter": "^1.1.0",
    "crlf": "^1.1.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.15",
    "log-symbols": "^3.0.0",
    "minimist": "^1.2.0",
    "snyk": "^1.256.0"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "xo": "^0.25.3"
  },
  "snyk": true
}
