{
  "name": "cfpathcheck",
  "type": "module",
  "description": "Check CFML files for correct paths in cfinclude/cfimport tags",
  "version": "5.0.0",
  "homepage": "https://github.com/timbeadle/cfpathcheck",
  "author": {
    "name": "Tim Beadle",
    "url": "https://github.com/timbeadle"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timbeadle/cfpathcheck.git"
  },
  "bugs": {
    "url": "https://github.com/timbeadle/cfpathcheck/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/timbeadle/cfpathcheck/blob/main/LICENSE-MIT"
    }
  ],
  "keywords": [
    "cli",
    "command line",
    "cfml",
    "static analysis"
  ],
  "preferGlobal": true,
  "bin": "./bin/cli.js",
  "main": "./lib/cfpathcheck.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "run-p test:snyk test:lint test:unit",
    "test:ci": "run-p test:lint test:unit",
    "test:coverage": "nyc --reporter=lcov npm run test:unit",
    "test:lint": "eslint {bin,lib,test}/*.js",
    "test:snyk": "snyk test --dev",
    "test:unit": "mocha test/*.spec.js",
    "snyk-protect": "snyk-protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@snyk/protect": "^1.865.0",
    "chalk": "^5.0.1",
    "checkstyle-formatter": "^1.1.0",
    "crlf": "^1.1.1",
    "deep-equal": "^2.0.5",
    "glob": "^7.2.0",
    "log-symbols": "^5.1.0",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/checkstyle-formatter": "^1.0.0",
    "@types/deep-equal": "^1.0.1",
    "@types/glob": "^7.2.0",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "eslint": "^8.10.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-plugin-import": "^2.25.4",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.6.1"
  },
  "snyk": true
}
